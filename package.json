{
	"name": "blog-server",
	"version": "1.0.0",
	"description": "blog-server",
	"private": true,
	"egg": {
		"typescript": true,
		"declarations": true
	},
	"scripts": {
		"start": "egg-scripts start --daemon --title=blog-server",
		"stop": "egg-scripts stop --title=blog-server",
		"dev": "egg-bin dev",
		"debug": "egg-bin debug",
		"test-local": "egg-bin test",
		"test": "npm run lint -- --fix && npm run test-local",
		"cov": "egg-bin cov",
		"tsc": "ets && tsc -p tsconfig.json",
		"ci": "npm run lint && npm run cov && npm run tsc",
		"autod": "autod",
		"lint": "tslint --project . -c tslint.json --fix",
		"clean": "ets clean",
		"tree": "echo '```' > tree.md && tree >> tree.md -I 'node_modules' -F",
		"start:prod": "EGG_SERVER_ENV=prod npm start",
		"deploy": "npm run tsc && npm run start:prod"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "pretty-quick --staged"
		}
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"egg": "^2.6.1",
		"egg-logrotator": "^3.1.0",
		"egg-mongoose": "^3.2.0",
		"egg-scripts": "^2.6.0",
		"egg-validate": "^2.0.2",
		"jsonwebtoken": "^8.5.1",
		"koa-compress": "^3.0.0",
		"moment": "^2.24.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@types/cheerio": "^0.22.15",
		"@types/jsonwebtoken": "^8.3.5",
		"@types/koa-compress": "^2.0.9",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.18",
		"@types/supertest": "^2.0.0",
		"autod": "^3.0.1",
		"autod-egg": "^1.1.0",
		"egg-bin": "^4.11.0",
		"egg-ci": "^1.8.0",
		"egg-mock": "^3.16.0",
		"husky": "^3.1.0",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"tslib": "^1.9.0",
		"tslint": "^5.0.0",
		"tslint-config-egg": "^1.0.0",
		"typescript": "^3.7.3"
	},
	"engines": {
		"node": ">=8.9.0"
	},
	"ci": {
		"version": "8"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"eslintIgnore": [
		"coverage"
	],
	"author": "BreathlessWay",
	"license": "MIT"
}
